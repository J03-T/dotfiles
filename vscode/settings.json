{
    "telemetry.telemetryLevel": "off",
    "zenMode.showTabs": "none",
    "zenMode.centerLayout": true,
    "window.menuBarVisibility": "toggle",
    "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font'",
    "editor.minimap.enabled": false,
    "dotnetAcquisitionExtension.enableTelemetry": false,
    "zenMode.hideLineNumbers": false,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false,
        "<C-k>": false,
        "<C-b>": false,
        "<C-j>": false,
        "<C-p>": false,
        "<C-i>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "v"
            ],
            "commands": [
                ":vsplit"
            ]
        },
        {
            "before": [
                "leader",
                "s"
            ],
            "commands": [
                ":split"
            ]
        },
        {
            "before": [
                "leader",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "leader",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "commands": [
                "workbench.action.focusAboveGroup"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "commands": [
                "workbench.action.focusBelowGroup"
            ]
        },
        {
            "before": [
                "leader",
                "q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "leader",
                "["
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                "leader",
                "]"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        }
    ],
    "vim.leader": "<Space>",
    "editor.lineNumbers": "relative",
    "haskell.manageHLS": "GHCup",
    "workbench.settings.editor": "json",
    "editor.rulers": [
        80
    ],
    "editor.fontFamily": "'Cascadia Code'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.scrollbar.vertical": "hidden",
    "breadcrumbs.enabled": false,
    "workbench.editorAssociations": {
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Everforest Dark",
    "workbench.editor.showTabs": "none",
    "rust-analyzer.inlayHints.closingBraceHints.enable": true,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
    "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
    "rust-analyzer.inlayHints.reborrowHints.enable": "never",
    "rust-analyzer.inlayHints.bindingModeHints.enable": false,
    "rust-analyzer.inlayHints.implicitDrops.enable": false,
    "rust-analyzer.inlayHints.closureCaptureHints.enable": true,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "window.customTitleBarVisibility": "windowed",
    "hediet.vscode-drawio.resizeImages": null,
    "editor.cursorSmoothCaretAnimation": "on",
    "files.simpleDialog.enable": true,
    "gitlens.telemetry.enabled": false,
    "gitlens.liveshare.enabled": true,
    "gitlens.currentLine.enabled": false,
    "editor.formatOnSave": true,
    "editor.bracketPairColorization.enabled": false,
    "cmake.configureOnOpen": true,
    "cmake.useCMakePresets": "always",
    "cmake.options.statusBarVisibility": "visible",
    "cmake.showOptionsMovedNotification": false,
    "C_Cpp.intelliSenseEngine": "disabled",
    "git.autofetch": true,
    "workbench.statusBar.visible": false,
    "workbench.activityBar.location": "hidden"
}